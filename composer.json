{
   "name": "pivlu/pivlu",
   "description": "Pivlu: Open source CMS and Website Builder.",
   "type": "library",
   "version": "2.0.0",
   "authors": [
      {
         "name": "Chimilevschi Iosif-Gabriel",
         "email": "office@pivlu.com"
      }
   ],
   "license": "AGPL-3.0-or-later",
   "homepage": "https://pivlu.com",
   "require": {
      "php": "^8.2",
      "laravel/framework": "^12.0",
      "laravel/fortify": "^1.33",
      "league/flysystem-aws-s3-v3": "^3.29",
      "spatie/laravel-medialibrary": "^11.17",
      "spatie/laravel-permission": "^6.23",
      "fakerphp/faker": "^1.24"
   },
   "autoload": {
      "psr-4": {
